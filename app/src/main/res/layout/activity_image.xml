<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="by.thedrop.newmath.Activities.ImageActivity">


    <ImageView
        android:id="@+id/image_activity_mainImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitStart"/>


    <uk.co.senab.photoview.PhotoView xmlns:android="http://schemas.android.com/apk/res/android"
                                     android:id="@+id/image_activity_photoView"
                                     android:layout_width="match_parent"
                                     android:layout_height="match_parent"/>
</RelativeLayout>
